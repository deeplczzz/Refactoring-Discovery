{
    "name": "refactoring-discovery",
    "version": "v1.0.1",
    "main": "electron.js",
    "scripts": {
        "start": "node ./bin/start",
        "build": "webpack --color --config webpack.config.js",
        "deploy": "npm run build && node writeHtml.js",
        "electron": "cross-env ELECTRON_START_URL=http://localhost:3001 electron .",
        "electron-build": "npm run build && electron .",
        "dist": "npm run build && electron-builder"
    },
    "dependencies": {
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "antd": "^4.24.16",
        "classnames": "^2.2.6",
        "diff": "^4.0.2",
        "ejs": "^2.4.1",
        "express": "^4.16.4",
        "http-errors": "^1.6.2",
        "morgan": "^1.9.1",
        "react": "^16.8.3",
        "react-dom": "^16.8.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-import": "^1.13.0",
        "cross-env": "^7.0.3",
        "css-loader": "^0.28.11",
        "electron": "^32.1.2",
        "electron-builder": "^24.6.0",
        "mini-css-extract-plugin": "^0.4.5",
        "source-map-loader": "^1.0.1",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3"
    },
    "build": {
        "appId": "com.refactoring-discovery",
        "files": [
            "dist/**/*",
            "electron.js",
            "server/**/*"
        ],
        "mac": {
            "category": "public.app-category.utilities"
        },
        "win": {
            "target": "nsis"
        },
        "linux": {
            "target": "AppImage"
        }
    }
}
